{
    "name": "starsrivers/flarum-mathren",
    "description": "TeX math rendering for your forum.",
    "keywords": [
        "flarum",
        "mathren",
        "mathematics",
        "expressions",
        "tex",
        "latex",
        "katex"
    ],
    "type": "flarum-extension",
    "license": "MIT",
    "authors": [{
        "name": "StarsRivers",
        "email": "weizixuan110@foxmail.com",
        "homepage": "https://github.com/StarsRivers/flarum-mathren/"
    }],
    "support": {
        "issues": "https://github.com/StarsRivers/flarum-mathren/issues",
        "source": "https://github.com/StarsRivers/flarum-mathren/"
    },
    "require": {
        "flarum/core": ">=0.1.0-beta.10"
    },
    "autoload": {
        "psr-4": {
            "TheTurk\\MathRen\\": "src/"
        }
    },
    "extra": {
        "flarum-extension": {
            "title": "MathRen",
            "icon": {
                "name": "fas fa-square-root-alt",
                "backgroundColor": "#b72a2a",
                "color": "#fff"
            }
        }
    }
}
